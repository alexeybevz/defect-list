        <DatePicker Name="DatePicker_Start" Margin="10,10,10,0" HorizontalAlignment="Center" VerticalAlignment="Top" Width="156" FirstDayOfWeek="Monday" FontSize="14"/>
        <DatePicker Name="DatePicker_End" Margin="10,41,10,0" HorizontalAlignment="Center" VerticalAlignment="Top" Width="156" FontSize="14"/>
        <Button x:Name="button_OK" Content="OK" VerticalAlignment="Top" Margin="18,73,99,0" Width="75" Click="button_OK_Click"/>
        <Button x:Name="button_Cancel" Content="Отмена" VerticalAlignment="Top" Width="75" Click="button_Cancel_Click" Margin="98,73,19,0"/>
    </Grid>
</Window>